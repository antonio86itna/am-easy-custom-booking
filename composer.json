{
  "name": "totaliweb/am-easy-custom-booking",
  "description": "Custom booking plugin for Costabilerent (WordPress + Elementor + Stripe + Mapbox).",
  "type": "wordpress-plugin",
  "license": "proprietary",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.13",
    "wp-coding-standards/wpcs": "^3.2"
  },
  "autoload": {
    "psr-4": { "AMCB\\": "src/" }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
